{
    "compilerOptions": {
        "sourceMap": true,
        "lib": [
            "es2015",
            "es6",
            "dom"
        ],
        "module": "commonjs",
        "baseUrl": ".",
        "paths": {
            "Business/*": [
                "./app/Business/*"
            ],
            "DinDin/*": [
                "./app/*"
            ],
            "Components/*": [
                "./app/Components/*"
            ],
            "UI/*": [
                "./app/UI/*"
            ],
            "Styles/*": [
                "./app/Styles/*"
            ]
        },
        "target": "es5",
        "jsx": "react"
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}