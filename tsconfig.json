{
  "compilerOptions": {
    "allowJs": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "lib": [
      "es2015",
      "es6",
      "dom"
    ],
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "Business/*": [
        "./app/Business/*"
      ],
      "Components/*": [
        "./app/Components/*"
      ],
      "UI/*": [
        "./app/UI/*"
      ],
      "Styles/*": [
        "./app/Styles/*"
      ],
      "DinDin/*": [
        "./app/*"
      ]
    },
    "target": "es5",
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}