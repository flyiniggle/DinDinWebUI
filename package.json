{
  "name": "dindinwebui",
  "version": "0.0.7",
  "description": "Web interface for DinDin",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --colors -p",
    "build-dev": "webpack --colors --watch --errors-only --open",
    "heroku-prebuild": "npm install --dev",
    "lint": "eslint --fix ./",
    "serve": "node server/server.js",
    "serve-dev": "nodemon server/server.js",
    "start": "node server/server.js",
    "start-dev": "run-p build-dev serve-dev",
    "storybook": "start-storybook -p 9001 -c storybook",
    "test": "jest --config test/jest.config.js",
    "test-ci": "jest --ci --coverage --config test/jest.config.js && cat ./test/coverage/lcov.info | coveralls",
    "test-dev": "jest --config test/jest.config.js --watch --verbose"
  },
